{
  "name": "my-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:e2e": "cypress run --browser chrome --headless",
    "test:e2e:debug": "node_modules/.bin/cypress open",
    "graphql:schema:cleanup": "rm -rf graphql/__generated__",
    "graphql:schema:download": "apollo schema:download --endpoint $HASURA_APOLLO_CLIENT_URI --header \"X-Hasura-Admin-Secret: $HASURA_ADMIN_SECRET\"",
    "graphql:schema:generate": "npm run graphql:schema:cleanup && npm run graphql:schema:download && apollo client:codegen --target typescript --localSchemaFile schema.json --includes graphql/**/*.ts --globalTypesFile graphql/__generated__/globalTypes.d.ts --tsFileExtension d.ts"
  },
  "dependencies": {
    "@apollo/client": "^3.3.20",
    "@auth0/nextjs-auth0": "^1.4.2",
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.4",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "framer-motion": "^4.1.17",
    "graphql": "^15.5.1",
    "next": "11.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-icons": "^4.2.0",
    "request": "^2.88.2",
    "swr": "^0.5.6"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/react": "^12.0.0",
    "@types/graphql": "^14.5.0",
    "@types/jest": "^26.0.23",
    "@types/react": "17.0.11",
    "@types/react-test-renderer": "^17.0.1",
    "@types/request": "^2.48.5",
    "@typescript-eslint/eslint-plugin": "^4.28.0",
    "@typescript-eslint/parser": "^4.28.0",
    "apollo": "^2.33.4",
    "cypress": "^7.6.0",
    "cypress-image-snapshot": "^4.0.1",
    "eslint": "^7.29.0",
    "eslint-config-next": "11.0.1",
    "eslint-config-standard": "^16.0.3",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "husky": "^6.0.0",
    "jest": "^27.0.5",
    "react-test-renderer": "^17.0.2",
    "typescript": "4.3.4"
  }
}